{
  "name": "fhir-pipeline",
  "version": "1.0.0",
  "description": "Event-driven healthcare data pipeline on GCP that harmonizes CSV and HL7v2 data into FHIR R4 format",
  "main": "dist/harmonization/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start:harmonization": "node dist/harmonization/index.js",
    "start:api": "node dist/api/index.js",
    "dev:harmonization": "ts-node src/harmonization/index.ts",
    "dev:api": "ts-node src/api/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "fhir",
    "healthcare",
    "gcp",
    "cloud-run",
    "hl7v2",
    "data-pipeline"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "@google-cloud/storage": "^7.7.0",
    "csv-parse": "^5.5.3",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
